{
  "name": "tmpcontainers",
  "version": "0.0.0",
  "description": "Temporary Containers",
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "build": "rm -rf web-ext-artifacts .cache dist && npm run lint && npm test && parcel build src/ui/options.html src/ui/popup.html --no-minify --no-source-maps && cp README.md LICENSE dist && web-ext build -s dist && addons-linter web-ext-artifacts/temporary_containers-*.zip && ncu",
    "build-sign": "rm -rf web-ext-artifacts .cache dist && node build-sign.js && npm run lint && npm test && parcel build src/ui/options.html src/ui/popup.html --no-minify --no-source-maps && cp README.md LICENSE dist && web-ext sign --channel unlisted -s dist && git checkout -- src/manifest.json && ncu",
    "lint": "eslint src/**/*.{js,vue}",
    "test": "nyc --reporter=html --reporter=text mocha --reporter=dot ./test/setup.js test/*.test.js --timeout 60000",
    "test-watch": "mocha ./test/setup.js test/*.test.js -b --watch --tmp-debug --inspect",
    "test-functional": "mocha ./test/functional/*.test.js --timeout 10000",
    "check-dependencies": "ncu",
    "watch": "rm -rf dist && parcel src/ui/options.html src/ui/popup.html --no-hmr",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stoically/temporary-containers.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stoically/temporary-containers/issues"
  },
  "homepage": "https://github.com/stoically/temporary-containers#readme",
  "dependencies": {
    "delay": "^4.3.0",
    "glob-to-regexp": "^0.4.1",
    "jquery": "^3.4.1",
    "jquery-address": "^1.6.0",
    "p-cancelable": "^2.0.0",
    "p-queue": "^3.0.0",
    "psl": "^1.1.33",
    "semantic-ui": "^2.4.2",
    "semver-compare": "^1.0.0",
    "sortablejs": "^1.10.0-rc3",
    "vue": "^2.6.10",
    "vuedraggable": "^2.23.0"
  },
  "devDependencies": {
    "@vue/component-compiler-utils": "^3.0.0",
    "addons-linter": "^1.10.0",
    "babel-eslint": "^10.0.2",
    "chai": "^4.2.0",
    "coveralls": "^3.0.4",
    "eslint": "^6.0.1",
    "eslint-plugin-vue": "^5.2.3",
    "mocha": "^6.1.4",
    "npm-check-updates": "^3.1.12",
    "nyc": "^14.1.1",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-static-files-copy": "stoically/parcel-plugin-static-files-copy#excludeGlob",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "vue-template-compiler": "^2.6.10",
    "vuepress": "^1.0.2",
    "web-ext": "^3.0.0",
    "webextensions-geckodriver": "^0.5.1",
    "webextensions-jsdom": "^0.16.0"
  },
  "browserslist": [
    "Firefox >= 60"
  ],
  "staticFiles": {
    "staticPath": [
      "src",
      "src/ui/static"
    ],
    "watcherGlob": "**",
    "excludeGlob": [
      "ui",
      "ui/**"
    ],
    "globOptions": {
      "dot": true
    }
  },
  "posthtml": {
    "recognizeSelfClosing": true
  },
  "private": true,
  "nyc": {
    "exclude": [
      "test/**/*.js"
    ]
  }
}
